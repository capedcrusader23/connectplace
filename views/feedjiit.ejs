    <script>
        new Vue({
            el:"#ed",
            data:{
                len:[],
                tmp:'',
                ques:[<% ques.forEach(function(x){%>
                    <%x.anser._id=0%>
                    {link2:'/users/'+'<%=x.ans%>',link:'/project/'+'<%=x.qid%>',content:'<%=x.topic%>',upvote:'<%=x.upvote.length%>',downvote:'<%=x.downvote.length%>',ans:'<%= x.ansby%>',project:'<%=x.qid%>',ansid:'<%=x.ans%>',cat:[ ],time:'<%=x.time%>' },
                  <%})%>]
            }
        });
